<view class="search_row">
  <input value="{{inputValue}}" bindinput="handleInput" placeholder="输入要搜索的商品"/>
  <button bindtap="handleCancel" hidden="{{!isFocus}}">取消</button>
</view>
<view class="search_content">
  <navigator
    class="search_item"
    wx:for="{{goods}}"
    wx:key="goods_id"
    bidntap="handle"
    url="/pages/goods_detail/index?goods_id={{item.goods_id}}">
    {{item.goods_name}}
  </navigator>
</view>